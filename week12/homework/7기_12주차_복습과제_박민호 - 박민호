    "def crawling(urls) :\n",
    "    browser = Chrome(executable_path = r'/Users/bobos_park/Downloads/Backup/Bitamin_Assign/12week/chromedriver') \n",
    "    names_prices_data = []\n",
    "    \n",
    "    for url in urls:\n",
    "        browser.get(url)\n",
    "\n",
    "        while browser.find_element_by_css_selector('.btn-next.disabled').text == '27':  # 이 부분을 무엇이라고 해야 마지막까지 갈 수 있을지 모르겠습니다..btn-next disabled가 27인 경우 계속 진행되게 짜놓았다고 생각합니다\n",
    "        #for j in range(int(last_p)):\n",
    "            # 마지막 요소뽑은거 텍스트가 last_p보다 작으면 반복 \n",
    "            sleep(0.01)\n",
    "            if browser.find_element_by_class_name('btn-next').text =='다음' :\n",
    "                sleep(0.01)\n",
    "\n",
    "\n",
    "                for j in range(27):\n",
    "                    try:\n",
    "                        browser.find_elements_by_class_name('btn-page')[j].click()\n",
    "                        sleep(0.01)\n",
    "                        for i in range(len(browser.find_elements_by_css_selector('.discriptions-inner'))):\n",
    "                            sleep(0.01)\n",
    "                            name_data = browser.find_elements_by_css_selector('.discriptions-inner.name')[i].text\n",
    "                            sleep(0.01)\n",
    "                            price_data = browser.find_elements_by_css_selector('.discriptions-inner.price-value')[i].text\n",
    "                            sleep(0.01)\n",
    "                        #comments.extend(browser.find_elements_by_css_selector('.u_cbox_contents')[i].text)\n",
    "                        #sleep(0.6)\n",
    "                            names_prices_data.append((name_data, price_data))\n",
    "                            print(names_prices_data)\n",
    "                    except:\n",
    "                        continue\n",
    "               \n",
    "\n",
    "            else :\n",
    "                sleep(0.01)\n",
    "                for j in range(len(browser.find_elements_by_class_name('btn-page'))):\n",
    "                    sleep(0.6)\n",
    "                    browser.find_elements_by_class_name('btn-page')[j].click()\n",
    "                    sleep(0.6)\n",
    "                    try:\n",
    "                        for i in range(len(browser.find_elements_by_css_selector('.price-value'))):\n",
    "                            sleep(0.01)\n",
    "                            name_data = browser.find_elements_by_css_selector('.name')[i].text\n",
    "                            price_data = browser.find_elements_by_css_selector('.price-value')[i].text\n",
    "                            sleep(0.01)\n",
    "                        #comments.extend(browser.find_elements_by_css_selector('.u_cbox_contents')[i].text)\n",
    "                        #sleep(0.6)\n",
    "                            names_prices_data.append((name_data,price_data))\n",
    "                            \n",
    "                    except :\n",
    "                        continue\n",
    "\n",
    "    return names_prices_data\n",
    "                             \n",
    "                             "
